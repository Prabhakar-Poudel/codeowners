{
  "name": "codeowner",
  "displayName": "codeowner",
  "description": "vew file codeowners and much more",
  "version": "1.0.0",
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Prabhakar-Poudel/codeowners"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "Prabhakar",
  "activationEvents": [
    "workspaceContains:**/CODEOWNERS"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codeowner.showFilesForOwner",
        "title": "Codeowner: List all files owned by someone/team"
      },
      {
        "command": "codeowner.fileOwners",
        "title": "Codeowner: Show owners of current file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.1.2",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.40.0",
    "glob": "^10.2.3",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "ignore": "^5.2.4"
  }
}
